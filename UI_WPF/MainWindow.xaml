<Window x:Class="UI_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="btnGoogleMap" Content="Call GoogleMap" HorizontalAlignment="Center" Margin="10,10,10,10" VerticalAlignment="Center" Width="145" Height="56" Click="btnGoogleMap_Click"/>
        <Button Content="Button"  Grid.Row="1" HorizontalAlignment="Center" Margin="10,10,10,10" VerticalAlignment="Center" Width="145" Height="56"/>
        <Button Content="Button" Grid.Row="2" HorizontalAlignment="Center" Margin="10,10,10,10" VerticalAlignment="Center" Width="145" Height="56"/>
    </Grid>
</Window>
